<body>
    <%- include("partials/navbar");%>

    <div class="cont">
        <div class="images box">
            <h1>'Tagline'</h1>
            <div class="images-cont">
                <div class="main-img"></div>
                <div class="sub-imgs">
                    <div class="img"></div>
                    <div class="img"></div>
                    <div class="img"></div>
                    <div class="img"></div>
                </div>
            </div>
        </div>
        <div class="dets">
            <div class="left">
                <div class="box">
                    <div class="dets-cards">
                        <div class="dets-card box">
                            <i class="fa-regular fa-clock"></i>
                            <h2>'duration'</h2>
                            <p>Duration</p>
                        </div>
                        <div class="dets-card box">
                            <i class="fa-solid fa-star"></i>
                            <h2>5.0</h2>
                            <p>Rating</p>
                        </div>
                        <div class="dets-card box">
                            <i class="fa-solid fa-pen"></i>
                            <h2>'count'</h2>
                            <p>Reviews</p>
                        </div>
                    </div>
                </div>
                <div class="desc box">
                    <h2>Description</h2>
